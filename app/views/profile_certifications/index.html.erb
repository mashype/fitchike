<p id="notice"><%= notice %></p>

<h1>Listing Profile Certifications</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profile_certifications.each do |profile_certification| %>
      <tr>
        <td><%= profile_certification.cert_year %></td>
        <td><%= link_to 'Show', profile_certification %></td>
        <td><%= link_to 'Edit', edit_profile_certification_path(profile_certification) %></td>
        <td><%= link_to 'Destroy', profile_certification, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile certification', new_profile_certification_path %>
